{
    "name": "bravepickle/simple-dto-bundle",
    "description": "Allow to configure dto objects in easy way, and split persistence and representative application layers",
    "license": "MIT",
    "keywords": [
        "symfony",
        "bundle",
        "dto",
        "data transfer object",
        "rest",
        "restful",
        "serialization"
    ],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Evgeniy Malykhin",
            "email": "55jeka@gmail.com"
        },
        {
            "name": "bravepickle",
            "email": "2bravepickle@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/framework-bundle": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/flex": "^1.0",
        "symfony/maker-bundle": "^1.4",
        "symfony/orm-pack": "^1.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/yaml": "^4.2",
        "symfony/validator": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.3.0"
    },
    "autoload": {
        "psr-4": {
            "Mell\\Bundle\\SimpleDtoBundle\\": ""
        }
    },
    "suggest": {
        "nelmio/NelmioApiDocBundle": "Generates documentation for your REST API from annotations"
    },
    "extra": {
        "symfony": {
            "require": "4.2.*"
        }
    }
}
